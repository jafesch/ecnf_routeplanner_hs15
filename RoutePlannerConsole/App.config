<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <sources>
      <source name="RoutesErrors" switchValue="Critical" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile"/>
          <add name="Console"/>
          <remove name="Default"/>
        </listeners>
      </source>
      <source name="CitiesErrors" switchValue="Information" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile"/>
          <add name="Console"/>
          <remove name="Default"/>
        </listeners>
      </source>
      <source name="Cities" switchValue="Information" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile"/>
          <remove name="Default"/>
        </listeners>
      </source>
      <source name="Routes" switchValue="Information" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile"/>
          <remove name="Default"/>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="LogFile"
        type="System.Diagnostics.TextWriterTraceListener"
        initializeData="routeplanner.log">
      </add>
      <add name="Console"
        type="System.Diagnostics.ConsoleTraceListener">
      </add>
    </sharedListeners>
  </system.diagnostics>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
</configuration>